              %>
              <% if (variant.salesPrice && variant.regularPrice && variant.salesPrice < variant.regularPrice) { %>
                <del>₹<%= variant.regularPrice %></del> ₹<%= variant.salesPrice %>
              <% } else { %>
                ₹<%= variant.regularPrice || 'N/A' %>
